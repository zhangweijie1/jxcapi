<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrguo.dao.bsd.StoreOptionsMapper">

    <select id="selectStoreAllOptions" resultType="com.mrguo.dto.OptionStore">
        select
        s.id as `value`, s.`name` as label, s.is_default, s.is_lock
        from bsd_store s
    </select>
</mapper>